
<form>
    <p>{{localize "DND5E.SpellCastUpcast" }}</p>
    <p class="notes"></p>
    <div class="form-group">
        <label>{{ localize "DND5E.SpellLevel" }}</label>
        <div class="form-fields">
            <select data-name="spellupcast" name="level">
                {{#select item.data.level}}
                {{#each spellLevels as |l|}}
                <option value="{{l.level}}">{{l.label}}</option>
                {{/each}}
                {{/select}}
            </select>
        </div>
    </div>
</form>